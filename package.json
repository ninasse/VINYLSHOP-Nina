{
  "name": "ninasvinylshop",
  "version": "0.0.1",
  "description": "A simple webshop",
  "main": "index.js",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "unittest": "mocha",
    "cypress": "cypress open",
    "cy:run": "cypress run -- --record --spec 'cypress/integration/todo_ui_spec.js'",
    "test": "npm run unittest & npm run cy:run",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./test,./npm-debug.log --copy-files,./sass",
    "sass": "node-sass --watch sass -o public",
    "start-dev": "nodemon index.js",
    "start-build": "npm run build && node dist/index.js",
    "start": "node index.js"
  },
  "keywords": [
    "webshop",
    "minimal",
    "FED19S",
    "vinylshop"
  ],
  "author": "Nina",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.2",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^6.4.5",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "querystring": "^0.2.0",
    "request": "^2.88.2",
    "stripe": "^8.38.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "cypress": "^4.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  }
}
